import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class ArrayMean {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
        }
        double sum = 0;
        for (int num : arr) {
            sum += num;
        }
        double mean = sum / n;
        System.out.printf("Array Mean Value is %.2f\n", mean);
    }
}
    
