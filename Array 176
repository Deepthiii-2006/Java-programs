import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class DoorNumberFinder {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] a = new int[n];
        for (int i = 0; i < n; i++) {
            a[i] = sc.nextInt();
        }
        int address = sc.nextInt();
        if (address <= 0) {
            System.out.println("-1");
        } else if (address > n) {
            System.out.println("Door Number is 000-DN");
        } else {
            int doorNum = a[address - 1];
            String formattedDoor = String.format("%03d", doorNum);
            System.out.println("Door Number is " + formattedDoor + "-DN");
        }
    }
}
